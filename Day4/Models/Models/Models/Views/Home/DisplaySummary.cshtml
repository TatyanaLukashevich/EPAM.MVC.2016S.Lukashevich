@using Models.Models
@model Address
@{
    ViewBag.Title = "DisplaySummary";
}

<h2>DisplaySummary</h2>

@using (Html.BeginForm("DisplaySummary", "Home"))
{
    <div class="row">
        @Html.LabelFor(m => m.City, new { @class = "col-xs-3" })
        @Html.EditorFor(m => m.City, new { @class = "col-xs-3" })
    </div>

    <div class="row">
        @Html.LabelFor(m => m.Country, new { @class = "col-xs-3" })
        @Html.EditorFor(m => m.Country, new { @class = "col-xs-3" })
    </div>

    <button type="submit">Submit</button>
}